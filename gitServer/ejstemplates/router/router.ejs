import <%= row.routerName %> from "../pages/<%= row.routerPath %>"
<%_ _%><% }) -%>
let routers = [
<%_ data.forEach(function(row){ -%>
    {
        path:"/<%= row.routerName %><% if(row.type == '2'){ %>/:tag/:id<% } %>",
        name:"<%= row.routerName %>",
        component:<%= row.routerName %>
    },
